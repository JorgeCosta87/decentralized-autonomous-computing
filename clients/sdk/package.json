{
  "name": "sdk",
  "scripts": {
    "build": "tsc",
    "ci": "npm run build",
    "dev": "esrun --watch src/index.ts",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint .",
    "start": "cross-env NODE_OPTIONS=--disable-warning=ExperimentalWarning tsx src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:run": "tsx src/test.ts",
    "list-ipfs": "tsx scripts/list-ipfs-files.ts"
  },
  "displayName": "Node Server",
  "description": "Node.js script with gill (based on @solana/kit)",
  "usecase": "Backend",
  "keywords": [
    "gill",
    "node",
    "solana-kit"
  ],
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.5.2",
    "@types/prompts": "^2.4.9",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.0",
    "@solana/kit": "^5.4.0",
    "@types/bs58": "^4.0.4",
    "bs58": "^6.0.0",
    "esrun": "^3.2.30",
    "js-yaml": "^4.1.0",
    "kleur": "^4.1.5",
    "prompts": "^2.4.2",
    "tsx": "^4.19.2",
    "ws": "^8.18.3"
  }
}
